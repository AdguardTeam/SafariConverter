{
  "name": "safari-converter",
  "version": "3.0.1",
  "description": "Safari Content Blocker Converter",
  "scripts": {
    "build": "gulp buildConverter --require babel-register --gulpfile tasks",
    "test": "gulp runTests --require babel-register --gulpfile tasks",
    "convert": "gulp runConvertation --require babel-register --gulpfile tasks",
    "extension": "gulp buildExtension --require babel-register --gulpfile tasks"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "babelrc": false
  },
  "author": "adguard@adguard.com",
  "license": "LGPL-3.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-download2": "^1.0.2",
    "gulp-file": "^0.4.0",
    "del":"^3.0.0",
    "node-qunit-phantomjs": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "qunit": "^2.6.0"
  }
}
